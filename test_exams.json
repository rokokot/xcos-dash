{
  "id": "exam-demo",
  "name": "Exam Scheduling Demo",
  "variables": [
    {"name": "Exam_A", "domain": ["Mon_9am", "Mon_2pm", "Tue_9am", "Tue_2pm"]},
    {"name": "Exam_B", "domain": ["Mon_9am", "Mon_2pm", "Tue_9am", "Tue_2pm"]},
    {"name": "Exam_C", "domain": ["Mon_9am", "Mon_2pm", "Tue_9am", "Tue_2pm"]},
    {"name": "Exam_D", "domain": ["Mon_9am", "Mon_2pm", "Tue_9am", "Tue_2pm"]}
  ],
  "constraints": [
    {
      "id": "c1",
      "expression": "AllDifferent([Exam_A, Exam_B, Exam_C, Exam_D])",
      "type": "hard",
      "description": "All exams at different times",
      "enabled": true
    }
  ],
  "metadata": {}
}
